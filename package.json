{
  "name": "ntsl",
  "displayName": "NTSL",
  "description": "NTSL Colorised for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ntsl",
      "aliases": ["Nanotrasen Scripting Language", "ntsl"],
      "extensions": [".ntsl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ntsl",
      "scopeName": "source.ntsl",
      "path": "./syntaxes/ntsl.tmLanguage.json"
    }]
  }
}
